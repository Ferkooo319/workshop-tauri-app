{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "workshop-tauri-app",
  "version": "0.0.0",
  "identifier": "wksp.feri.io",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "workshop-tauri-app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://cdn.crabnebula.app/update/my-awesome-org/my-awesome-app/{{target}}-{{arch}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI1MjUyMTNEMEM4OUEwNEUKUldST29Ja01QU0VsSlJCckxNQmwvZ29KMkFCcGdRUFNQNzQ4ZDlXWWtPOWZIc3Nnd0t0ZDhlYTQK"
    }
  },
  "bundle": {
    "macOS": {
      "signingIdentity": "-"
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
